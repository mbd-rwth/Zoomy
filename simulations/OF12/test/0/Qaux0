FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Qaux0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    side_wall
    {
        type            zeroGradient;
    }
    bottom_wall
    {
        type            zeroGradient;
    }
}



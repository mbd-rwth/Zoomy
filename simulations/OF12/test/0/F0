FoamFile
{
    format      ascii;
    class       surfaceScalarField;
    location    "0";
    object      F0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    side_wall
    {
        type            calculated;
        value           uniform 0;
    }
    bottom_wall
    {
        type            calculated;
        value           uniform 0;
    }
}



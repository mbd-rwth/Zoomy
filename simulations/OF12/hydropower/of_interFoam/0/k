FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.0001841496;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    side_wall
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    bottom_wall
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    top_wall
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //


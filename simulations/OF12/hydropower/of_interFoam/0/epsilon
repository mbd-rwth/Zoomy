FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.000078079233;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    side_wall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    bottom_wall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    top_wall
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //


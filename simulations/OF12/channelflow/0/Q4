FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Q4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    front_wall
    {
        type            empty;
    }
    back_wall
    {
        type            empty;
    }
    inflow
    {
        type            fixedValue;
        value           uniform 0;
    }
    outflow
    {
        type            fixedValue;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}



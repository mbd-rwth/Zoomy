FoamFile
{
    format      ascii;
    class       surfaceScalarField;
    location    "0";
    object      F0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    front_wall
    {
        type            calculated;
        value           uniform 0;
    }
    back_wall
    {
        type            calculated;
        value           uniform 0;
    }
    inflow
    {
        type            calculated;
        value           uniform 0;
    }
    outflow
    {
        type            calculated;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}


